{
  "name": "neorecrut-monorepo",
  "version": "1.0.0",
  "packageManager": "npm@10.9.3",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "dev:neo": "turbo run dev --filter=neo",
    "build": "turbo run build",
    "build:neo": "turbo run build --filter=neo",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rmdir /s /q node_modules"
  },
  "devDependencies": {
    "turbo": "^2.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}